    #include<iostream>
    using namespace std;
     
     
    int main(){
     
     
    int t;
    int arr[10000];
    cin>>t;
     
    while(t--){
     
        int n;
        cin>>n;
        int sum1 = 0;
        int sum2 = 0;
        for(int i =0;i<n;i++){
            cin>>arr[i];
            if(arr[i] == 1)sum1++;
            else sum2++;
        }
     
         if(sum1 %2 != 0){
            cout<<"NO"<<endl;
            continue;
         }
     
         if(sum1 == 0 && sum2 %2 !=0)
            cout<<"NO"<<endl;
         else cout<<"yes"<<endl;
     
     
    }
     
    }

