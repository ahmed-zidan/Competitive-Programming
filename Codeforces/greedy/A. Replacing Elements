#include<iostream>
#include<algorithm>
using namespace std;

int main(){

int t;
cin>>t;
int arr[1000];
while(t--){

    int n,d;
    cin>>n>>d;
    for(int i =0;i<n;i++)
        cin>>arr[i];
    sort(arr,arr+n);
    if(arr[0]+arr[1] <= d || arr[n-1] <= d)
        cout<<"yes"<<endl;
    else cout<<"no"<<endl;



}






}
