#include<iostream>
using namespace std;
int main(){


int t;
cin>>t;



while(t--){

char red[1001];
char blue[1001];

    int n;
    cin>>n;
    for(int i =0;i<n;i++)cin>>red[i];
    for(int i =0;i<n;i++)cin>>blue[i];

    int b = 0;
    int r = 0;
    for(int i =0;i<n;i++){
        if(red[i]>blue[i])r++;
        else if(red[i]<blue[i])b++;
    }

    if(r > b)cout<<"RED"<<endl;
    else if(r<b)cout<<"BLUE"<<endl;
    else cout<<"EQUAL"<<endl;


}





}
