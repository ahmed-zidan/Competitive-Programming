class Solution(object):
    def duplicateZeros(self, arr):
        """
        :type arr: List[int]
        :rtype: None Do not return anything, modify arr in-place instead.
        """
        dest = arr[:]
        d=0
        for i in dest:
            if(i == 0):
                arr[d] = 0
                d+=1
                if(d>=len(arr)):
                    break
                arr[d] =0
                d+=1
            else: arr[d]=i;d+=1
                
            if(d>=len(arr)):
                break
            
        
        
        
        
            
                
  
